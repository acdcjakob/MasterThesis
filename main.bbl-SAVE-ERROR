      \field{issn}{1931-9401}
      \field{journaltitle}{Applied Physics Reviews}
      \field{langid}{english}
      \field{month}{3}
      \field{number}{1}
      \field{title}{A review of Ga{\textsubscript{2}}O{\textsubscript{3}} materials, processing, and devices}
      \field{volume}{5}
      \field{year}{2018}
      \field{dateera}{ce}
