      \field{issn}{0022-3727, 1361-6463}
      \field{journaltitle}{Journal of Physics D: Applied Physics}
      \field{langid}{english}
      \field{month}{12}
      \field{number}{49}
      \field{shortjournal}{J. Phys. D: Appl. Phys.}
      \field{title}{Effects of sapphire substrate orientation on Sn-doped {$\alpha$}-Ga{\textsubscript{2}}O{\textsubscript{3}} grown by halide vapor phase epitaxy using {$\alpha$}-Cr{\textsubscript{2}}O{\textsubscript{3}} buffers}
