      \strng{namehash}{47a7e01dded591898eaabfc13f8e441a}
      \strng{fullhash}{551cc17ace5ffc818fd1cd74f2ecf6ce}
      \strng{fullhashraw}{551cc17ace5ffc818fd1cd74f2ecf6ce}
