    \entry{lebreau2014}{article}{}{}
      \name{author}{4}{}{%
        {{hash=85d2196a686f76a34a993121e5377e69}{%
           family={Lebreau},
           familyi={L\bibinitperiod},
           given={François},
           giveni={F\bibinitperiod}}}%
        {{hash=9c1f30722e48c95b57679b1ef489c60f}{%
