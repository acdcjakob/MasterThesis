      \strng{bibnamehash}{f7c9596608da813d0167b6a2bd86916d}
      \strng{authorbibnamehash}{f7c9596608da813d0167b6a2bd86916d}
      \strng{authornamehash}{f7c9596608da813d0167b6a2bd86916d}
