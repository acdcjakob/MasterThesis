      \strng{fullhash}{9c0205ba0d96e98d7435d87aa025fce7}
      \strng{fullhashraw}{9c0205ba0d96e98d7435d87aa025fce7}
      \strng{bibnamehash}{51c686882907816f73698fd92a779bb4}
      \strng{authorbibnamehash}{51c686882907816f73698fd92a779bb4}
      \strng{authornamehash}{51c686882907816f73698fd92a779bb4}
