      \strng{namehash}{0bf088c252e084a3d1367f86b703bd76}
      \strng{fullhash}{0bf088c252e084a3d1367f86b703bd76}
      \strng{fullhashraw}{0bf088c252e084a3d1367f86b703bd76}
      \strng{bibnamehash}{0bf088c252e084a3d1367f86b703bd76}
      \strng{authorbibnamehash}{0bf088c252e084a3d1367f86b703bd76}
