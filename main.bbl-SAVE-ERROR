      \strng{fullhash}{2368fdc0446c8bc0ca87bd55db7b7d7d}
      \strng{fullhashraw}{2368fdc0446c8bc0ca87bd55db7b7d7d}
      \strng{bibnamehash}{5e1b1b2556ccc66a9eb2c810163eda2f}
      \strng{authorbibnamehash}{5e1b1b2556ccc66a9eb2c810163eda2f}
