      \strng{fullhashraw}{c12da78f1066aa68ecb2db216227e388}
      \strng{bibnamehash}{e3126a639bf4f8a8c24daf6a7e8be010}
      \strng{authorbibnamehash}{e3126a639bf4f8a8c24daf6a7e8be010}
